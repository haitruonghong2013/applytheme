<%= simple_form_for(@major_variance_log) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :meeting_id %>
    <%= f.input :datetime %>
    <%= f.input :variance %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :created_by %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
