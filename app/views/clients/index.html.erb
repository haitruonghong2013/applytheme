<h1>Listing clients</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Phone</th>
    <th>Email</th>
    <th>Country</th>
    <th>City</th>
    <th>Address</th>
    <th>Age</th>
    <th>Sex</th>
    <th>Create_By</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @clients.each do |client| %>
  <tr>
    <td><%= client.name %></td>
    <td><%= client.phone %></td>
    <td><%= client.email %></td>
    <td><%= client.country %></td>
    <td><%= client.city %></td>
    <td><%= client.address %></td>
    <td><%= client.age %></td>
    <td><%= client.sex %></td>
    <td><%= client.user ? client.user.username : '' %></td>
    <td><%= link_to 'Show', client %></td>
    <td><%= link_to 'Edit', edit_client_path(client) %></td>
    <td><%= link_to 'Destroy', client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Client', new_client_path %>
