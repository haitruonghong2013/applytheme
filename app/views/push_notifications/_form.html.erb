<%= simple_form_for(@push_notification) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :device_id %>
    <%= f.input :notif %>
    <%= f.input :reminder_for_next_meeting %>
    <%= f.input :reminder_for_take_note %>
    <%= f.input :time_reminder_befor_meeting %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
