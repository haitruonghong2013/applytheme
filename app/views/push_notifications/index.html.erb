<h1>Listing push_notifications</h1>

<table>
  <tr>
    <th>Device</th>
    <th>Notif</th>
    <th>Reminder for next meeting</th>
    <th>Time reminder befor meeting</th>
    <th>Reminder for take note</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @push_notifications.each do |push_notification| %>
  <tr>
    <td><%= push_notification.device_id %></td>
    <td><%= push_notification.notif %></td>
    <td><%= push_notification.reminder_for_next_meeting %></td>
    <td><%= push_notification.time_reminder_befor_meeting %></td>
    <td><%= push_notification.reminder_for_take_note %></td>
    <td><%= link_to 'Show', push_notification %></td>
    <td><%= link_to 'Edit', edit_push_notification_path(push_notification) %></td>
    <td><%= link_to 'Destroy', push_notification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Push notification', new_push_notification_path %>
