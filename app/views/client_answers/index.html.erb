<h1>Listing client_answers</h1>

<table>
  <tr>
    <th>Client</th>
    <th>Question</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @client_answers.each do |client_answer| %>
  <tr>
    <td><%= client_answer.client_id %></td>
    <td><%= client_answer.question_id %></td>
    <td><%= client_answer.content %></td>
    <td><%= link_to 'Show', client_answer %></td>
    <td><%= link_to 'Edit', edit_client_answer_path(client_answer) %></td>
    <td><%= link_to 'Destroy', client_answer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

