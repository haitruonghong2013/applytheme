<h1>Listing staff_tracking_positions</h1>

<table>
  <tr>
    <th>User</th>
    <th>Schedule</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @staff_tracking_positions.each do |staff_tracking_position| %>
  <tr>
    <td><%= staff_tracking_position.user_id %></td>
    <td><%= staff_tracking_position.schedule_id %></td>
    <td><%= staff_tracking_position.latitude %></td>
    <td><%= staff_tracking_position.longitude %></td>
    <td><%= link_to 'Show', staff_tracking_position %></td>
    <td><%= link_to 'Edit', edit_staff_tracking_position_path(staff_tracking_position) %></td>
    <td><%= link_to 'Destroy', staff_tracking_position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

