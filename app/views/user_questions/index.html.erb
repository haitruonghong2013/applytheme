<h1>Listing user_questions</h1>

<table>
  <tr>
    <th>User</th>
    <th>Question</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_questions.each do |user_question| %>
  <tr>
    <td><%= user_question.user_id %></td>
    <td><%= user_question.question_id %></td>
    <td><%= user_question.content %></td>
    <td><%= link_to 'Show', user_question %></td>
    <td><%= link_to 'Edit', edit_user_question_path(user_question) %></td>
    <td><%= link_to 'Destroy', user_question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

